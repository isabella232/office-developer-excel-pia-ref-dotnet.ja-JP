    <Member MemberName="Add2">
      <MemberSignature Language="C#" Value="public Microsoft.Office.Interop.Excel.SlicerCache Add2 (object Source, object SourceField, object Name, object SlicerCacheType);" />
      <MemberSignature Language="ILAsm" Value=".method public hidebysig newslot virtual instance class Microsoft.Office.Interop.Excel.SlicerCache Add2([in]object Source, [in]object SourceField, [in]object Name, [in]object SlicerCacheType) runtime managed" />
